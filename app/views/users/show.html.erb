<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user, size: 60 %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>

<div class="container">
<h3 class="center"><%= @user.email %></h3>
  <h5>Created at: <%= @user.created_at %></h5>
  <h5>Updated at: <%= @user.updated_at %></h5>
  <h5>Time now: <%=  Time.now %></h5>
</div>
